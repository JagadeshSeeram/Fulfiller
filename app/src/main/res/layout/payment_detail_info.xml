<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DADADA"
    android:orientation="vertical">
    <include layout="@layout/toolbar_search"></include>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_15"
        android:padding="@dimen/dp_10"
        android:background="@color/white"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:background="@drawable/editext_bottom"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/dp_10"
            android:paddingLeft="@dimen/dp_5"
            android:paddingRight="@dimen/dp_5"
            android:paddingTop="5dp"
            android:weightSum="1">


            <com.biglynx.fulfiller.fonts.CustomFontMedium
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center|start"
                android:layout_weight="0.4"
                android:gravity="center|start"
                android:maxLines="1"
                android:text="Fulfillment ID :"
                android:textColor="@color/header_color"
                android:textSize="@dimen/sp_13"></com.biglynx.fulfiller.fonts.CustomFontMedium>

            <com.biglynx.fulfiller.fonts.CustomFontLight
                android:id="@+id/fulfillmentid_tv"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center|end"
                android:layout_weight="0.6"
                android:gravity="center|end"
                android:maxLines="1"
                android:singleLine="true"
                android:textColor="#000"
                android:textSize="@dimen/sp_11"></com.biglynx.fulfiller.fonts.CustomFontLight>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:background="@drawable/editext_bottom"
            android:orientation="horizontal"
            android:paddingTop="10dp"
            android:paddingBottom="@dimen/dp_10"
            android:paddingRight="@dimen/dp_5"
            android:paddingLeft="@dimen/dp_5"
            android:weightSum="1">


            <com.biglynx.fulfiller.fonts.CustomFontMedium
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center|start"
                android:layout_weight="0.4"
                android:gravity="center|start"
                android:maxLines="1"
                android:text="Retailer name :"
                android:textColor="@color/header_color"
                android:textSize="@dimen/sp_13"></com.biglynx.fulfiller.fonts.CustomFontMedium>

            <com.biglynx.fulfiller.fonts.CustomFontLight
                android:id="@+id/retailer_name_tv"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center|end"
                android:layout_weight="0.6"
                android:gravity="center|end"
                android:maxLines="1"
                android:singleLine="true"
                android:textColor="#000"
                android:textSize="@dimen/sp_11"></com.biglynx.fulfiller.fonts.CustomFontLight>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:background="@drawable/editext_bottom"
            android:orientation="horizontal"
            android:paddingTop="10dp"
            android:paddingBottom="@dimen/dp_10"
            android:paddingRight="@dimen/dp_5"
            android:paddingLeft="@dimen/dp_5"
            android:weightSum="1">


            <com.biglynx.fulfiller.fonts.CustomFontMedium
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center|start"
                android:layout_weight="0.5"
                android:gravity="center|start"
                android:maxLines="1"
                android:text="Interest expressed date :"
                android:textColor="@color/header_color"
                android:textSize="@dimen/sp_13"></com.biglynx.fulfiller.fonts.CustomFontMedium>

            <com.biglynx.fulfiller.fonts.CustomFontLight
                android:id="@+id/interest_date__tv"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center|end"
                android:layout_weight="0.5"
                android:gravity="center|end"
                android:maxLines="1"
                android:singleLine="true"
                android:textColor="#000"
                android:textSize="@dimen/sp_11"></com.biglynx.fulfiller.fonts.CustomFontLight>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:background="@drawable/editext_bottom"
            android:orientation="horizontal"
            android:paddingTop="10dp"
            android:paddingBottom="@dimen/dp_10"
            android:paddingRight="@dimen/dp_5"
            android:paddingLeft="@dimen/dp_5"
            android:weightSum="1">


            <com.biglynx.fulfiller.fonts.CustomFontMedium
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center|start"
                android:layout_weight="0.5"
                android:gravity="center|start"
                android:maxLines="1"
                android:text="Orders delivered :"
                android:textColor="@color/header_color"
                android:textSize="@dimen/sp_13"></com.biglynx.fulfiller.fonts.CustomFontMedium>

            <com.biglynx.fulfiller.fonts.CustomFontLight
                android:id="@+id/orders_delivered_tv"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center|end"
                android:layout_weight="0.5"
                android:gravity="center|end"
                android:maxLines="1"
                android:singleLine="true"
                android:textColor="#000"
                android:textSize="@dimen/sp_11"></com.biglynx.fulfiller.fonts.CustomFontLight>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:background="@drawable/editext_bottom"
            android:orientation="horizontal"
            android:paddingTop="10dp"
            android:paddingBottom="@dimen/dp_10"
            android:paddingRight="@dimen/dp_5"
            android:paddingLeft="@dimen/dp_5"
            android:weightSum="1">


            <com.biglynx.fulfiller.fonts.CustomFontMedium
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center|start"
                android:layout_weight="0.65"
                android:gravity="center|start"
                android:maxLines="1"
                android:text="Total weight of deliver :"
                android:textColor="@color/header_color"
                android:textSize="@dimen/sp_13"></com.biglynx.fulfiller.fonts.CustomFontMedium>

            <com.biglynx.fulfiller.fonts.CustomFontLight
                android:id="@+id/total_weight_tv"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center|end"
                android:layout_weight="0.35"
                android:gravity="center|end"
                android:maxLines="1"
                android:singleLine="true"
                android:textColor="#000"
                android:textSize="@dimen/sp_11"></com.biglynx.fulfiller.fonts.CustomFontLight>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:background="@drawable/editext_bottom"
            android:orientation="horizontal"
            android:paddingTop="10dp"
            android:paddingBottom="@dimen/dp_10"
            android:paddingRight="@dimen/dp_5"
            android:paddingLeft="@dimen/dp_5"
            android:weightSum="1">


            <com.biglynx.fulfiller.fonts.CustomFontMedium
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center|start"
                android:layout_weight="0.4"
                android:gravity="center|start"
                android:maxLines="1"
                android:text="Total pay out :"
                android:textColor="@color/header_color"
                android:textSize="@dimen/sp_13"></com.biglynx.fulfiller.fonts.CustomFontMedium>

            <com.biglynx.fulfiller.fonts.CustomFontLight
                android:id="@+id/total_payout_tv"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center|end"
                android:layout_weight="0.6"
                android:gravity="center|end"
                android:maxLines="1"
                android:singleLine="true"
                android:textColor="#000"
                android:textSize="@dimen/sp_11"></com.biglynx.fulfiller.fonts.CustomFontLight>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:orientation="horizontal"
            android:paddingTop="10dp"
            android:paddingBottom="@dimen/dp_5"
            android:paddingRight="@dimen/dp_5"
            android:paddingLeft="@dimen/dp_5"
            android:weightSum="1">


            <com.biglynx.fulfiller.fonts.CustomFontMedium
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center|start"
                android:layout_weight="0.4"
                android:gravity="center|start"
                android:maxLines="1"
                android:text="Payment status :"
                android:textColor="@color/header_color"
                android:textSize="@dimen/sp_13"></com.biglynx.fulfiller.fonts.CustomFontMedium>

            <com.biglynx.fulfiller.fonts.CustomFontLight
                android:id="@+id/payment_status_tv"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center|end"
                android:layout_weight="0.6"
                android:gravity="center|end"
                android:maxLines="1"
                android:singleLine="true"
                android:textColor="#000"
                android:textSize="@dimen/sp_11"></com.biglynx.fulfiller.fonts.CustomFontLight>
        </LinearLayout>

    </LinearLayout>
</LinearLayout>